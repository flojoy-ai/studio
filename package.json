{
  "name": "flojoy-desktop",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "electron/main.js",
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@mantine/core": "^5.10.4",
    "@mantine/hooks": "^5.10.4",
    "@tabler/icons": "^1.71.0",
    "@tisoap/react-flow-smart-edge": "^3.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "http-proxy-middleware": "^2.0.6",
    "immer": "^9.0.19",
    "ioredis": "^4.28.5",
    "jotai": "^2.0.2",
    "jotai-immer": "^0.2.0",
    "js-yaml": "^4.1.0",
    "just-clone": "^6.0.1",
    "localforage": "^1.10.0",
    "module": "^1.2.5",
    "plotly.js": "^2.17.0",
    "rc-slider": "^9.7.5",
    "react": "^17.0.2",
    "react-7-segment-display": "^1.1.1",
    "react-dom": "^17.0.2",
    "react-grid-layout": "^1.3.4",
    "react-modal": "^3.14.4",
    "react-plotly.js": "^2.6.0",
    "react-resizable": "^3.0.4",
    "react-scripts": "5.0.0",
    "react-select": "^5.3.0",
    "react-switch": "^7.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-tabs": "^3.2.3",
    "react-use": "^17.4.0",
    "reactflow": "^11.4.2",
    "redis": "^4.0.4",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "styled-components": "^5.3.3",
    "use-file-picker": "^1.4.2",
    "uuid": "^8.3.2",
    "wait-on": "^6.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "e2e": "CI=true node_modules/.bin/cypress run --headed --browser chrome",
    "eject": "react-app-rewired eject",
    "cypress": "cypress open",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-package": "npm run build && electron-builder build --publish always -mwl",
    "electron-publish": "electron-builder build --publish always -mwl",
    "test": "CI=true node_modules/.bin/jest",
    "test:watch": "jest --watch --coverage"
  },
  "build": {
    "appId": "io.flojoy.desktop",
    "productName": "Flojoy Desktop",
    "asar": false,
    "files": [
      "build/**/*",
      "electron/main.js",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "./docker-compose-prod.yml",
        "to": "./docker-compose-prod.yml"
      }
    ],
    "mac": {
      "target": [
        "dmg"
      ],
      "darkModeSupport": true
    },
    "linux": {
      "target": [
        "tar.gz"
      ]
    },
    "win": {
      "target": [
        "portable"
      ]
    },
    "extraMetadata": {
      "main": "electron/main.js"
    },
    "extends": null
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/snapshot": "^2.1.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@types/node": "^18.0.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/react-grid-layout": "^1.3.2",
    "@types/react-modal": "^3.13.1",
    "@types/react-plotly.js": "^2.5.2",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/uuid": "^8.3.4",
    "concurrently": "^7.4.0",
    "cypress": "^10.11.0",
    "dotenv": "^16.0.3",
    "electron": "^21.0.1",
    "electron-builder": "^23.3.3",
    "eslint-plugin-cypress": "^2.12.1",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "react-app-rewired": "^2.2.1",
    "react-dial-knob": "^1.3.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.9.3"
  }
}
