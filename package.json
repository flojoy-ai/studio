  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "backend": "poetry run python3 manage.py",
    "backend:debug": "poetry run python3 manage.py --log-level debug",
    "bundle-plotly": "cd node_modules/plotly.js && npm i && npm run custom-bundle -- --traces scatter,scatter3d,surface,table,histogram,image,bar,indicator,heatmap",
    "start-project": "concurrently --prefix \"[{time}-{name}]\" -n \"React,FastAPI\" -c auto  \"npm:start\" \"npm:backend\"",
    "start-project:win": "concurrently --prefix \"[{time}-{name}]\" -n \"React,FastAPI\" -c auto \"npm:start\" \"python manage.py\"",
    "start-project:win:debug": "concurrently --prefix \"[{time}-{name}]\" -n \"React,FastAPI\" -c auto \"npm:start\" \"python manage.py --log-level debug\"",
    "start-project:ci": "CI=true concurrently --prefix \"[{time}-{name}]\" -n \"React,FastAPI\" -c auto  \"npm:start\" \"npm:backend\"",
    "start-project:debug": "DEBUG=true concurrently --prefix \"[{time}-{name}]\" -n \"React,FastAPI\" -c auto  \"npm:start\" \"npm:backend:debug\"",
    "format": "npx prettier --write . && poetry run black . --extend-exclude '/(node_modules)/'",
    "check": "npx prettier --check . && poetry run black . --extend-exclude '/(node_modules)/' --check",
    "e2e": "CI=true cypress run --browser chrome",
    "cypress": "cypress open",
    "electron-dev": "electron .",
    "electron-package": "npm run build && electron-builder build --publish always -wml",
    "electron-package:win": "npm run build && electron-builder build --win",
    "electron-package:linux": "npm run build && electron-builder build --linux",
    "electron-package:mac": "npm run build && electron-builder build --mac --universal",
    "electron-publish": "electron-builder build --publish always -mwl",
    "test": "CI=true jest",
    "test:watch": "jest --watch --coverage",
    "test:dev": "jest --updateSnapshot",
    "update-submodule": "git submodule update --remote --recursive",
    "cy:run:report": "nyc --reporter=html cypress run"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.1",
    "@electron/notarize": "^2.1.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@percy/cli": "^1.27.1",
