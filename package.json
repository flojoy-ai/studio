  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "extension": [
      ".tsx",
      ".ts"
    ],
    "include": [
      "src/**/*.tsx",
      "src/**/*.ts"
    ],
    "exclude": [
      "src/hooks/__mocks__",
      "src/utils/__mocks__",
      "src/feature/controls_panel/",
      "src/feature/results_panel/",
      "src/web-socket",
      "src/vite-env.d.ts"
    ],
    "all": true
  }
