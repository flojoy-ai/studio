{
  "name": "flojoy-desktop",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "cors": "^2.8.5",
    "electron-is-dev": "^2.0.0",
    "express": "^4.17.3",
    "file-saver": "^2.0.5",
    "http-proxy-middleware": "^2.0.6",
    "immer": "^9.0.15",
    "ioredis": "^4.28.5",
    "jotai": "^1.7.3",
    "js-yaml": "^4.1.0",
    "just-clone": "^6.0.1",
    "localforage": "^1.10.0",
    "module": "^1.2.5",
    "plotly.js": "^2.11.0",
    "rc-slider": "^9.7.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-flow-renderer": "^9.7.4",
    "react-grid-layout": "^1.3.4",
    "react-modal": "^3.14.4",
    "react-plotly.js": "^2.5.1",
    "react-resizable": "^3.0.4",
    "react-scripts": "5.0.0",
    "react-select": "^5.3.0",
    "react-switch": "^7.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-tabs": "^3.2.3",
    "react-use": "^17.4.0",
    "redis": "^4.0.4",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "styled-components": "^5.3.3",
    "typescript": "^4.7.4",
    "use-file-picker": "^1.4.2",
    "uuid": "^8.3.2",
    "wait-on": "^6.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "node_modules/.bin/cypress run",
    "eject": "react-scripts eject",
    "cypress": "cypress open",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-package": "npm run build && electron-builder build --publish always -mwl",
    "electron-publish": "electron-builder build --publish always -mwl"
  },
  "build": {
    "appId": "io.flojoy.desktop",
    "productName": "Flojoy Desktop",
    "asar": false,
    "files": [
      "build/**/*",
      "electron/main.js",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "./docker-compose-prod.yml",
        "to": "./docker-compose-prod.yml"
      }
    ],
    "mac": {
      "target": ["dmg"],
      "darkModeSupport": true
    },
    "linux": {
      "target": ["tar.gz"]
    },
    "win": {
      "target": ["portable"]
    },
    "extraMetadata": {
      "main": "electron/main.js"
    },
    "extends": null
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:cypress/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-grid-layout": "^1.3.2",
    "@types/react-modal": "^3.13.1",
    "@types/react-plotly.js": "^2.5.2",
    "@types/uuid": "^8.3.4",
    "concurrently": "^7.4.0",
    "cypress": "^10.7.0",
    "electron": "^21.0.1",
    "electron-builder": "^23.3.3",
    "eslint-plugin-cypress": "^2.12.1",
    "react-dial-knob": "^1.3.0"
  }
}
