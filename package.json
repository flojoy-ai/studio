    "include": [
      "src/**/*.tsx",
      "src/**/*.ts"
    ],
    "exclude": [
      "src/feature/controls_panel/",
      "src/feature/results_panel/",
      "src/web-socket",
      "src/vite-env.d.ts"
    ],
    "all": true
  }
