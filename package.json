{
  "name": "flojoy-studio",
  "productName": "Flojoy Studio",
  "description": "Joyful visual programming for Python",
  "version": "0.1.24",
  "private": true,
  "homepage": "./",
  "author": "Jack Parmer <jp@flojoy.io>",
  "main": "out/main/index.js",
  "scripts": {
    "start": "electron-vite preview",
    "postinstall": "electron-builder install-app-deps",
    "dev": "electron-vite dev --inspect --watch",
    "build": "tsc && electron-vite build",
    "backend": "poetry run python3 main.py",
    "backend:debug": "poetry run python3 main.py --log-level debug",
    "bundle-plotly": "cd node_modules/plotly.js && pnpm install && pnpm run custom-bundle -- --traces scatter,scatter3d,surface,table,histogram,image,bar,indicator,heatmap",
    "start-project": "concurrently --prefix \"[{time}-{name}]\" -n \"React,FastAPI\" -c auto  \"pnpm:start\" \"pnpm:backend\"",
    "start-project:win": "concurrently --prefix \"[{time}-{name}]\" -n \"React,FastAPI\" -c auto \"pnpm:start\" \"python manage.py\"",
    "start-project:win:debug": "concurrently --prefix \"[{time}-{name}]\" -n \"React,FastAPI\" -c auto \"pnpm:start\" \"python manage.py --log-level debug\"",
    "start-project:ci": "CI=true concurrently --prefix \"[{time}-{name}]\" -n \"React,FastAPI\" -c auto  \"pnpm:start\" \"pnpm:backend\"",
    "start-project:debug": "DEBUG=true concurrently --prefix \"[{time}-{name}]\" -n \"React,FastAPI\" -c auto  \"pnpm:start\" \"pnpm:backend:debug\"",
    "format": "npx prettier --write .",
    "check": "npx prettier --check .",
    "e2e": "npx playwright test",
    "electron-dev": "electron .",
    "electron-package": "pnpm run build && electron-builder build -m --universal -lw",
    "electron-package:windows": "pnpm run build && electron-builder build --win",
    "electron-package:linux": "pnpm run build && electron-builder build --linux",
    "electron-package:mac": "pnpm run build && electron-builder build --mac --universal",
    "electron-publish": "electron-builder build --publish always -mwl",
    "electron:test": "npx playwright test"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.0.5",
